                        <lg justify="left">
                            <l n="bb735.1.ms.170.03" justify="left">O that</l>
                            <l n="bb735.1.ms.170.04" justify="left">men would</l>
                            <l n="bb735.1.ms.170.05" justify="left">seek immortal</l>
                            <l n="bb735.1.ms.170.06" justify="left">moments</l>
